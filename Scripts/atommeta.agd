          ELSE
            IF CANGORIGHT
              SPRITERIGHT
            ENDIF
          ENDIF
          FALL
        ENDREPEAT
      ENDIF
      LET I X
    ENDSPRITE
    EXPLODE 2
    REMOVE
    EXIT
