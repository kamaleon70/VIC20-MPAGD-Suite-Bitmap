  IF COLLISION 5		; If hit by weapon
    LET I DIRECTION
    OTHER
      SUBTRACT 1 FROM SETTINGA	; Decrement hitcounter
      IF SETTINGA = 0
        REMOVE
        SPAWN 1,19		; Spawn worm
        LET Y 200

        IF G > 0		; Decrement enemy nr if in combat room
          SUBTRACT 1 FROM G
