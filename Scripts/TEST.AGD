EVENT PLAYER

IF KEY LEFT
    LET DIRECTION = LEFT
    LET IMAGE = 0
    IF X <= LEFTEDGE
        SCREENLEFT
        LET X = RIGHTEDGE
        EXIT
    ELSE
        IF CANGOLEFT
            SPRITELEFT
            ANIMATE
        ENDIF
    ENDIF
ENDIF

IF KEY RIGHT
    LET DIRECTION = RIGHT
    LET IMAGE = 0
    IF X >= RIGHTEDGE
        SCREENRIGHT
        LET X = LEFTEDGE
        EXIT
    ELSE
        IF CANGORIGHT
            SPRITERIGHT
            ANIMATE
        ENDIF
    ENDIF
ENDIF

IF KEY FIRE
    JUMP 7
ENDIF
FALL

IF DEADLY
    KILL
ENDIF

GETBLOCKS



EVENT SPRITETYPE1

IF DIRECTION = UP
    IF CANGOUP
        SPRITEUP
    ELSE
        LET DIRECTION = DOWN
    ENDIF
ELSE
    IF CANGODOWN
        SPRITEDOWN
    ELSE
        LET DIRECTION = UP
    ENDIF
ENDIF

ANIMATE

IF COLLISION PLAYER
    KILL
ENDIF



EVENT SPRITETYPE2

IF DIRECTION = LEFT
    IF CANGOLEFT
        SPRITELEFT
        SUBTRACT 16 FROM X
        IF CANGODOWN
            LET DIRECTION = RIGHT
        ENDIF
        ADD 16 TO X
    ELSE
        LET DIRECTION = RIGHT
    ENDIF
ELSE
    IF CANGORIGHT
        SPRITERIGHT
        ADD 16 TO X
        IF CANGODOWN
            LET DIRECTION = LEFT
            LET IMAGE = 2
            LET FRAME = 0
        ENDIF
        SUBTRACT 16 FROM X
    ELSE
        LET DIRECTION = LEFT
        LET IMAGE = 2
        LET FRAME = 0
    ENDIF
ENDIF

ANIMATE

IF COLLISION PLAYER
    KILL
ENDIF


