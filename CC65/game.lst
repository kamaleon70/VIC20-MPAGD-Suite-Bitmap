000096r 3               	bne :+
000096r 3               	jmp star2 		; yes, it's right.
000096r 3               :
000096r 3               	cmp #2 			; is it up?
000096r 3               	bne :+
000096r 3               	jmp star3 		; yes, it's up.
000096r 3               :
000096r 3               	ldy wntopx 		; get edge of screen.
000096r 3               	iny			; down one pixel.
000096r 3               	tya
000096r 3               star8:
