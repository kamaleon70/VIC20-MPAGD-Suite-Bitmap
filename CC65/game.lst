001007r 3               	lda #32			; convert to a space instead.
001007r 3               scrly5:
001007r 3               	sta fntaddr		; calculate char address
001007r 3               	lda #0
001007r 3               	sta fntaddr+1
001007r 3               	asl fntaddr  		; multiply char by 8.
001007r 3               	rol fntaddr+1
001007r 3               	asl fntaddr
001007r 3               	rol fntaddr+1
001007r 3               	asl fntaddr
001007r 3               	rol fntaddr+1
001007r 3               	lda fntaddr
001007r 3               	clc
001007r 3               	adc #<(FONT-256)
