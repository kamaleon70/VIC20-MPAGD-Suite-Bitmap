000096r 3               
000096r 3               ;----------------------------------------------------
000096r 3               ; Run the routine
000096r 3               ;----------------------------------------------------
000096r 3               
000096r 3               prosh2:
000096r 3               	asl a 			; 2 bytes per address.
000096r 3               	tay
000096r 3               	lda shrptr,y
000096r 3               	sta z80_l
000096r 3               	lda shrptr+1,y 		; fetch high byte from table.
000096r 3               	sta z80_h
000096r 3               	jmp (z80_hl) 		; jump to routine.
