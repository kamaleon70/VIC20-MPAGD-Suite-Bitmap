000FEDr 3               ; Effects code.
000FEDr 3               ; Ticker routine is called 25 times per second.
000FEDr 3               ;
000FEDr 3               ; txtini = text scroller address
000FEDr 3               ; txtscr = left text screen address
000FEDr 3               ; txtwid = scroller width
000FEDr 3               ; txtbit = 128
000FEDr 3               ; X      = message nr
000FEDr 3               ;----------------------------------------------------------
000FEDr 3               
000FEDr 3               .if sflag
000FEDr 3               
000FEDr 3               scrollpos_lb:	.byte $00,$10,$20,$30,$40,$50,$60,$70
000FEDr 3               		.byte $80,$90,$a0,$b0,$c0,$d0,$e0,$f0
000FEDr 3               		.byte $00,$10,$20,$30,$40,$50
000FEDr 3               scrollpos_hb:	.byte $00,$00,$00,$00,$00,$00,$00,$00
000FEDr 3               		.byte $00,$00,$00,$00,$00,$00,$00,$00
000FEDr 3               		.byte $01,$01,$01,$01,$01,$01
000FEDr 3               
000FEDr 3               scrly:
000FEDr 3               
000FEDr 3               ; Set scr_l = txtscr
000FEDr 3               
000FEDr 3               	rts
000FEDr 3               	.word txtscr         	; set scr_l = left screen address.
000FEDr 3               	sta scr_l
000FEDr 3               	sta scr_r
000FEDr 3               	lda txtscr+1
000FEDr 3               	sta scr_l+1
000FEDr 3               	sta scr_r+1
000FEDr 3               
000FEDr 3               	stx xtmp		; save x
000FEDr 3               
000FEDr 3               ; Calculate scr_r = scr_l + 16*(txtwid -1)
000FEDr 3               
000FEDr 3               	ldx txtwid
000FEDr 3               	dex
000FEDr 3               	lda scrollpos_lb,x
000FEDr 3               	clc
000FEDr 3               	adc scr_r
000FEDr 3               	sta scr_r		; set lb scr_r = scr_l + (txtwid - 1) * 8
000FEDr 3               	bcc :+
000FEDr 3               	inc scr_r+1
000FEDr 3               :
000FEDr 3               	clc
000FEDr 3               	lda scrollpos_hb,x
000FEDr 3               	adc scr_r+1
000FEDr 3               	sta scr_r+1
000FEDr 3               
000FEDr 3               ; Scroll string
000FEDr 3               
000FEDr 3               	lda #0
000FEDr 3               	sta line_cnt		; Set line counter
000FEDr 3               scrly1:
000FEDr 3               	clc
000FEDr 3               	lda scr_r
000FEDr 3               	adc line_cnt
000FEDr 3               	sta scr_tmp
000FEDr 3               	lda scr_r+1
000FEDr 3               	sta scr_tmp+1
000FEDr 3               
000FEDr 3               	ldx txtwid		; set column counter
000FEDr 3               	dex
000FEDr 3               	ldy #0
000FEDr 3               	clc			; Reset pixel to be shifted in
000FEDr 3               scrly0:
000FEDr 3               	lda (scr_tmp),y
000FEDr 3               	rol a			; Shift screenbyte
000FEDr 3               	sta (scr_tmp),y		; store screenbyte
000FEDr 3               	php			; save carry
000FEDr 3               
000FEDr 3               	sec			; subtract 16 form screenaddress
000FEDr 3               	lda scr_tmp
000FEDr 3               	sbc #$10
000FEDr 3               	sta scr_tmp
000FEDr 3               	bcs :+
000FEDr 3               	dec scr_tmp+1
000FEDr 3               :
000FEDr 3               	plp			; restore carry
000FEDr 3               	dex			; previous column
000FEDr 3               	bpl scrly0		; repeat for all columns
000FEDr 3               
000FEDr 3               	inc line_cnt		; next line
000FEDr 3               	lda line_cnt
000FEDr 3               	cmp #8
000FEDr 3               	bne scrly1		; repeat for 8 lines
000FEDr 3               
000FEDr 3               ; Get next character in string
000FEDr 3               
000FEDr 3               	lda txtpos 		; get text pointer.
000FEDr 3               	sta scr_txt
000FEDr 3               	lda txtpos+1
000FEDr 3               	sta scr_txt+1
000FEDr 3               
000FEDr 3               	ldy #0
000FEDr 3               	lda (scr_txt),y 	; find character we're displaying.
000FEDr 3               	and #127 		; remove end marker bit if applicable.
000FEDr 3               	cmp #13			; is it newline?
000FEDr 3               	bne scrly5 		; no, it's okay.
000FEDr 3               	lda #32			; convert to a space instead.
000FEDr 3               scrly5:
000FEDr 3               	sta fntaddr		; calculate char address
000FEDr 3               	lda #0
000FEDr 3               	sta fntaddr+1
000FEDr 3               	asl fntaddr  		; multiply char by 8.
000FEDr 3               	rol fntaddr+1
000FEDr 3               	asl fntaddr
000FEDr 3               	rol fntaddr+1
000FEDr 3               	asl fntaddr
000FEDr 3               	rol fntaddr+1
000FEDr 3               	lda fntaddr
000FEDr 3               	clc
000FEDr 3               	adc #<(FONT-256)
000FEDr 3               	sta scrly3+1		; that's the low byte.
000FEDr 3               	lda fntaddr+1
000FEDr 3               	adc #>(FONT-256)
000FEDr 3               	sta scrly3+2		; add displacement.
000FEDr 3               
000FEDr 3               	ldy #0
000FEDr 3               scrly3:
000FEDr 3               	lda $3333,y		; get image of char line.
000FEDr 3               	and txtbit
000FEDr 3               	beq scrly2		; don't plot pixel
000FEDr 3               	lda (scr_r),y
000FEDr 3               	clc
000FEDr 3               	ora #1
000FEDr 3               	sta (scr_r),y		; plot pixel
000FEDr 3               scrly2:
000FEDr 3               	iny			; next line of char.
000FEDr 3               	cpy #8
000FEDr 3               	bne scrly3
000FEDr 3               
000FEDr 3               	lsr txtbit		; bit of text to display.
000FEDr 3               	bcs :+
000FEDr 3               	rts
000FEDr 3               :
000FEDr 3               	ldy #0
000FEDr 3               	lda (scr_txt),y 	; what was the character?
000FEDr 3               	asl a	  		; end of message?
000FEDr 3               	bcs scrly4
000FEDr 3               	inc txtpos
000FEDr 3               	bne :+
000FEDr 3               	inc txtpos+1
000FEDr 3               :
000FEDr 3               	jmp scrly6 		; not yet - continue.
000FEDr 3               scrly4:
000FEDr 3               	lda txtini 		; start of scrolling message.
000FEDr 3               	sta txtpos
000FEDr 3               	lda txtini+1
000FEDr 3               	sta txtpos+1
000FEDr 3               scrly6:
000FEDr 3               	lda #128
000FEDr 3               	sta txtbit
000FEDr 3               	ldx xtmp
000FEDr 3               	rts
