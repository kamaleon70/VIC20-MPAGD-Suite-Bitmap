001005r 3               scrollpos_lb:	.byte $00,$10,$20,$30,$40,$50,$60,$70
001005r 3               		.byte $80,$90,$a0,$b0,$c0,$d0,$e0,$f0
001005r 3               		.byte $00,$10,$20,$30,$40,$50
001005r 3               scrollpos_hb:	.byte $00,$00,$00,$00,$00,$00,$00,$00
