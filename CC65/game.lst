000FFFr 3               	plp			; restore carry
000FFFr 3               	dex			; previous column
000FFFr 3               	bpl scrly0		; repeat for all columns
000FFFr 3               
000FFFr 3               	inc line_cnt		; next line
000FFFr 3               	lda line_cnt
