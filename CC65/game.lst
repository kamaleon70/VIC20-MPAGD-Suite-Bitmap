000290r 3               ;  A = object number
000290r 3               ;-----------------------------------------------------------------
000290r 3               .if oflag .or mflag
000290r 3               gotob:
000290r 3               	cmp numob 		; number of objects in game.
000290r 3               	bcc :+ 			; are we checking past the end?
000290r 3               	jmp gotob0 		; yes, we can't have a non-existent object.
000290r 3               :
000290r 3               	jsr findob		; find the object.
000290r 3               gotob1:
000290r 3               	rts
000290r 3               
000290r 3               gotob0:
000290r 3               	lda #254 		; missing.
000290r 3               	jmp gotob1
000290r 3               
000290r 3               findob:
000290r 3               	pha			; save object number
000290r 3               	lda #<objdta 		; objects.
000290r 3               	sta z80_l
000290r 3               	lda #>objdta
000290r 3               	sta z80_h
000290r 3               	pla			; retreive object number
000290r 3               	beq fndob1 		; is it zero? yes, skip loop.
000290r 3               	tax 			; loop counter
000290r 3               fndob2:
000290r 3               	clc
000290r 3               	lda z80_l
000290r 3               	adc #39 		; size of each object.
000290r 3               	sta z80_l
000290r 3               	bcc :+
000290r 3               	inc z80_h
000290r 3               :
000290r 3               	dex 			; repeat until we find address.
000290r 3               	bne fndob2
000290r 3               fndob1:
000290r 3               	ldy #33			; distance to room it's in.
000290r 3               	lda (z80_hl),y		; fetch status.
000290r 3               	rts
000290r 3               .endif
000290r 3               
000290r 3               ;---------------------------------------------
000290r 3               ; Drop object number at (dispx, dispy).
000290r 3               ;
000290r 3               ; Input:
000290r 3               ;  A = object number
000290r 3               ;---------------------------------------------
000290r 3               
000290r 3               .if oflag
000290r 3               drpob:
000290r 3               	cmp numob 		; are we checking past the end?
000290r 3               	bcc :+
000290r 3               	rts			; yes, can't drop non-existent item.
000290r 3               :
000290r 3               	jsr gotob		; make sure object is in inventory.
000290r 3               	cmp scno		; already on this screen?
000290r 3               	bne :+
000290r 3               	rts			; yes, nothing to do.
000290r 3               :
000290r 3               	ldy #33
000290r 3               	lda scno
000290r 3               	sta (z80_hl),y		; bring onto screen.
000290r 3               	lda dispy		; sprite y coordinate.
000290r 3               	iny
000290r 3               	sta (z80_hl),y		; point to object y.
000290r 3               	lda dispx 		; sprite x coordinate.
000290r 3               	iny
000290r 3               	sta (z80_hl),y 		; point to object x
