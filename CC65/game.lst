0002A4r 3               
0002A4r 3               	ldy #35			; point to x coordinate now.
0002A4r 3               	lda (z80_hl),y 		; get coordinate.
0002A4r 3               	sec
0002A4r 3               	ldy #var_newX
0002A4r 3               	sbc (z80_ix),y 		; subtract the sprite x.
