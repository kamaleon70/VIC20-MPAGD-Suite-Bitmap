000082r 3               proshr:
000082r 3               .if pflag
000082r 3               	lda #<SHRAPN		; table.
000082r 3               	sta z80_x
000082r 3               	lda #>SHRAPN
000082r 3               	sta z80_i
000082r 3               
000082r 3               	lda #NUMSHR		; shrapnel pieces to process.
000082r 3               	sta shrctr
000082r 3               prosh0:
000082r 3               	ldy #0
000082r 3               	lda (z80_ix),y		; on/off marker.
000082r 3               	asl a
000082r 3               proshx:
000082r 3               	bcs :+
000082r 3               	jsr prosh1 		; on, so process it.
000082r 3               :
000082r 3               	clc
000082r 3               	lda z80_x
000082r 3               	adc #SHRSIZ
000082r 3               	sta z80_x
000082r 3               	bcc :+
000082r 3               	inc z80_i
000082r 3               :
000082r 3               	dec shrctr		; round again.
000082r 3               	bne prosh0
000082r 3               .endif
000082r 3               .if sflag
000082r 3               	jsr scrly
000082r 3               .endif
000082r 3  60           	rts
000083r 3               
000083r 3               .if pflag
000083r 3               ;----------------------------------------------------
000083r 3               ; Proces shrapnel piece
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               prosh1:
000083r 3               	jsr plot 		; delete the pixel.
000083r 3               
000083r 3               	lda #<shrptr		; shrapnel routine pointers.
000083r 3               	sta z80_l
000083r 3               	lda #>shrptr
000083r 3               	sta z80_h
000083r 3               
000083r 3               	ldy #0
000083r 3               	lda (z80_ix),y		; restore shrapnel type.
000083r 3               	jsr prosh2 		; run the routine.
000083r 3               	jsr chkxy		; check x and y are good before we redisplay.
000083r 3               
000083r 3               	lda #<SHRSIZ 		; distance to next.
000083r 3               	sta z80_e
000083r 3               	lda #>SHRSIZ
000083r 3               	sta z80_d
000083r 3               	rts
000083r 3               
000083r 3               ;----------------------------------------------------
000083r 3               ; Run the routine
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               prosh2:
000083r 3               	asl a 			; 2 bytes per address.
000083r 3               	tay
000083r 3               	lda shrptr,y
000083r 3               	sta z80_l
000083r 3               	lda shrptr+1,y 		; fetch high byte from table.
000083r 3               	sta z80_h
000083r 3               	jmp (z80_hl) 		; jump to routine.
