001007r 3               scrly0:
001007r 3               	lda (scr_tmp),y
001007r 3               	rol a			; Shift screenbyte
001007r 3               	sta (scr_tmp),y		; store screenbyte
001007r 3               	php			; save carry
001007r 3               
001007r 3               	sec			; subtract 16 form screenaddress
