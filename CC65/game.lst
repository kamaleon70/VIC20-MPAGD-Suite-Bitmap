000FFFr 3               
000FFFr 3               	stx xtmp		; save x
000FFFr 3               
000FFFr 3               ; Calculate scr_r = scr_l + 16*(txtwid -1)
000FFFr 3               
000FFFr 3               	ldx txtwid
000FFFr 3               	dex
000FFFr 3               	lda scrollpos_lb,x
000FFFr 3               	clc
000FFFr 3               	adc scr_r
