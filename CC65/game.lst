000083r 3               
000083r 3               ;----------------------------------------------------
000083r 3               ; Check if coordinates are ok before redrawing at new position.
000083r 3               ;
000083r 3               ; left:   X>L		X=L	Ok
000083r 3               ; right:  R+15>X	X=R	Ok
000083r 3               ; top:    Y>T		Y=T	Ok
000083r 3               ; bottom: B+15>Y	Y=B	Ok
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               chkxy:
000083r 3               
000083r 3               ; top:    Y>T		Y=T	Ok
000083r 3               
000083r 3               	ldy #3
000083r 3               	lda (z80_ix),y	 	; fetch shrapnel coordinate.
000083r 3               	cmp wntopx		; window top.
000083r 3               	bcs :+			; compare with top window limit.
000083r 3               	jmp kilshr		; out of window, kill shrapnel.
000083r 3               :
000083r 3               ; left:   X>L		X=L	Ok
000083r 3               
000083r 3               	ldy #5
000083r 3               	lda (z80_ix),y	 	; fetch shrapnel coordinate.
000083r 3               	cmp wnlftx		; left edge.
000083r 3               	bcs :+			; compare with left window limit.
000083r 3               	jmp kilshr		; out of window, kill shrapnel.
000083r 3               :
000083r 3               ; bottom: B+15>Y	Y=B	Ok
000083r 3               
000083r 3               	lda wnbotx		; point to bottom.
000083r 3               	clc
000083r 3               	adc #15
000083r 3               	ldy #3
000083r 3               	cmp (z80_ix),y	 	; fetch shrapnel coordinate.
000083r 3               	bcs :+			; compare with shrapnel x coordinate.
000083r 3               	jmp kilshr		; off screen, kill shrapnel..
000083r 3               :
000083r 3               ; right:  R+15>X	X=R	Ok
000083r 3               
000083r 3               	lda wnrgtx		; point to right edge.
000083r 3               	clc
000083r 3               	adc #15
000083r 3               	ldy #5
000083r 3               	cmp (z80_ix),y	 	; fetch shrapnel coordinate.
000083r 3               	bcs :+			; compare with window limit.
000083r 3               	jmp kilshr		; off screen, kill shrapnel.
000083r 3               :
000083r 3               
000083r 3               ;----------------------------------------------------
000083r 3               ; Drop through.
000083r 3               ; Display shrapnel.
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               plot:
000083r 3               	ldy #3
000083r 3               	lda (z80_ix),y		; y integer.
000083r 3               	sta dispy	 	; workspace coordinates.
000083r 3               	ldy #5
000083r 3               	lda (z80_ix),y	 	; x integer.
000083r 3               	sta dispx 		; workspace coordinates.
000083r 3               
000083r 3               	ldy #0
000083r 3               	lda (z80_ix),y 		; type.
000083r 3               	bne :+			; is it a laser?
000083r 3               	jmp plot1 		; yes, draw laser instead.
000083r 3               :
000083r 3               plot0:
000083r 3               	lda dispx		; which pixel within byte do we
