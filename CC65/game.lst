001005r 3               	cpy #8
001005r 3               	bne scrly3
001005r 3               
001005r 3               	lsr txtbit		; bit of text to display.
001005r 3               	bcs :+
001005r 3               	rts
001005r 3               :
001005r 3               	ldy #0
001005r 3               	lda (scr_txt),y 	; what was the character?
001005r 3               	asl a	  		; end of message?
001005r 3               	bcs scrly4
001005r 3               	inc txtpos
001005r 3               	bne :+
001005r 3               	inc txtpos+1
001005r 3               :
001005r 3               	jmp scrly6 		; not yet - continue.
001005r 3               scrly4:
