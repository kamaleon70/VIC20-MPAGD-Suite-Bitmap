000097r 3               ; Particle engine.
000097r 3               ;
000097r 3               ; Init particle data for 55 particles in SHRAPN table.
000097r 3               ; Every particle has 6 bytes.
000097r 3               ;
000097r 3               ; global:	-
000097r 3               ; local:	x,y,hl
000097r 3               ; calls:	-
000097r 3               ;------------------------------------------------------
000097r 3               
000097r 3               inishr:
000097r 3               	lda #<SHRAPN 		; table.
000097r 3               	sta z80_l
000097r 3               	lda #>SHRAPN
000097r 3               	sta z80_h
000097r 3               
000097r 3               	ldy #0
000097r 3               	ldx #NUMSHR		; shrapnel pieces to process.
000097r 3               inish0:
000097r 3               	lda #255 		; kill the shrapnel.
000097r 3               	sta (z80_hl),y
000097r 3               
000097r 3               	clc 			; point there.
000097r 3               	lda z80_l
000097r 3               	adc #SHRSIZ		; distance to next.
000097r 3               	sta z80_l
000097r 3               	bcc :+
000097r 3               	inc z80_h
000097r 3               :
000097r 3               	dex
000097r 3               	bne inish0 		; round again.
000097r 3               	rts
000097r 3               
000097r 3               ;------------------------------------------------------
000097r 3               ; Check for collision between laser and sprite.
000097r 3               ;------------------------------------------------------
000097r 3               
000097r 3               lcol:
000097r 3               	lda #<SHRAPN		; shrapnel table.
000097r 3               	sta z80_l
000097r 3               	lda #>SHRAPN
000097r 3               	sta z80_h
000097r 3               
000097r 3               	lda #NUMSHR		; number of pieces in table.
000097r 3               	sta z80_b
000097r 3               lcol0:
000097r 3               	ldy #0
000097r 3               	lda (z80_hl),y 		; get type.
000097r 3               	beq lcol1		; yes, check collision.
000097r 3               lcol3:
000097r 3               	clc			; point to more shrapnel.
000097r 3               	lda z80_l
000097r 3               	adc #SHRSIZ
000097r 3               	sta z80_l
000097r 3               	bcc :+
000097r 3               	inc z80_h
000097r 3               :
000097r 3               	dec z80_b		; repeat for all shrapnel.
000097r 3               	bne lcol0
000097r 3               	clc
000097r 3               	rts 			; no collision, carry not set.
000097r 3               lcol1:
000097r 3               	ldy #3
000097r 3               	lda (z80_hl),y		; get y.
000097r 3               	sec
000097r 3               	ldy #8
000097r 3               	sbc (z80_ix),y		; subtract sprite y.
000097r 3               lcolh:
000097r 3               	cmp #SPR_HGT 		; within range?
000097r 3               	bcc :+
000097r 3               	jmp lcol2		; no, missed.
000097r 3               :
000097r 3               	ldy #5
000097r 3               	lda (z80_hl),y 		; get x.
000097r 3               	sec
000097r 3               	ldy #9
000097r 3               	sbc (z80_ix),y 		; subtract sprite y.
000097r 3               	cmp #16			; within range?
000097r 3               	bcs :+
000097r 3               	jmp lcol4 		; yes, collision occurred.
000097r 3               :
000097r 3               lcol2:
000097r 3               	jmp lcol3
000097r 3               lcol4:
000097r 3               	sec
000097r 3               	rts 			; return with carry set for collision.
000097r 3               .endif
