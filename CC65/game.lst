001004r 3               
001004r 3               ; Dig right.
001004r 3               
001004r 3               digr:
001004r 3               	ldy #8
001004r 3               	lda (z80_ix),y
001004r 3               	sta dispy		; set y
001004r 3               
001004r 3               	iny
001004r 3               	lda (z80_ix),y
001004r 3               	clc
001004r 3               	adc #16
001004r 3               	sta dispx		; set x+16
