000083r 3               ;----------------------------------------------------
000083r 3               ; Explosion shrapnel.
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               shrap:
000083r 3               	ldy #1
000083r 3               	lda (z80_ix),y 		; get the angle.
000083r 3               	clc
000083r 3               	adc #<shrsin		; shrapnel sine table.
000083r 3               	sta z80_l
000083r 3               	lda #>shrsin
000083r 3               	adc #0
000083r 3               	sta z80_h
000083r 3               
000083r 3               	ldy #0
000083r 3               	lda (z80_hl),y 		; fetch value from table.
000083r 3               	sta z80_e
000083r 3               	inc z80_l 		; next byte of table.
000083r 3               	bne :+
000083r 3               	inc z80_h
000083r 3               :
000083r 3               	ldy #0
000083r 3               	lda (z80_hl),y		; fetch value from table.
000083r 3               	sta z80_d
000083r 3               	inc z80_l		; next byte of table.
000083r 3               	bne :+
000083r 3               	inc z80_h
000083r 3               :
000083r 3               	ldy #0
000083r 3               	lda (z80_hl),y 		; fetch value from table.
000083r 3               	sta z80_c
000083r 3               	inc z80_l 		; next byte of table.
000083r 3               	bne :+
000083r 3               	inc z80_h
000083r 3               :
000083r 3               	ldy #0
000083r 3               	lda (z80_hl),y 		; fetch value from table.
000083r 3               	sta z80_b
000083r 3               
000083r 3               	ldy #2
000083r 3               	lda (z80_ix),y 		; x coordinate in hl.
000083r 3               	clc
000083r 3               	adc z80_e		; add sine lb
000083r 3               	sta (z80_ix),y		; store new coordinate lb.
000083r 3               	ldy #3
000083r 3               	lda (z80_ix),y
000083r 3               	adc z80_d		; add sine hb
000083r 3               	sta (z80_ix),y		; store new coordinate hb.
000083r 3               
000083r 3               	ldy #4
000083r 3               	lda (z80_ix),y	 	; y coordinate in hl.
000083r 3               	clc
000083r 3               	adc z80_c		; add cosine lb
000083r 3               	sta (z80_ix),y		; store new coordinate lb.
000083r 3               	ldy #5
000083r 3               	lda (z80_ix),y
000083r 3               	adc z80_b		; add cosine lb
000083r 3               	sta (z80_ix),y		; store new coordinate hb.
000083r 3               
000083r 3               	rts
000083r 3               
000083r 3               ;----------------------------------------------------
000083r 3               ; Move dots
000083r 3               ;----------------------------------------------------
000083r 3               
000083r 3               dotl:
000083r 3               	ldy #5
000083r 3               	lda (z80_ix),y
000083r 3               	sec
000083r 3               	sbc #1		 	; move left.
000083r 3               	sta (z80_ix),y
000083r 3               	rts
000083r 3               dotr:
000083r 3               	ldy #5
000083r 3               	lda (z80_ix),y
000083r 3               	clc
000083r 3               	adc #1		 	; move left.
000083r 3               	sta (z80_ix),y
000083r 3               	rts
000083r 3               dotu:
000083r 3               	ldy #3
000083r 3               	lda (z80_ix),y
000083r 3               	sec
000083r 3               	sbc #1		 	; move up.
000083r 3               	sta (z80_ix),y
000083r 3               	rts
000083r 3               dotd:
000083r 3               	ldy #3
000083r 3               	lda (z80_ix),y
