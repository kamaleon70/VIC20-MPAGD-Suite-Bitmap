001005r 3               	clc
001005r 3               	lda scrollpos_hb,x
001005r 3               	adc scr_r+1
001005r 3               	sta scr_r+1
001005r 3               
001005r 3               ; Scroll string
001005r 3               
001005r 3               	lda #0
001005r 3               	sta line_cnt		; Set line counter
001005r 3               scrly1:
001005r 3               	clc
001005r 3               	lda scr_r
001005r 3               	adc line_cnt
001005r 3               	sta scr_tmp
001005r 3               	lda scr_r+1
001005r 3               	sta scr_tmp+1
001005r 3               
001005r 3               	ldx txtwid		; set column counter
