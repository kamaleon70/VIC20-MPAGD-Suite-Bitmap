001005r 3               scrly0:
001005r 3               	lda (scr_tmp),y
001005r 3               	rol a			; Shift screenbyte
001005r 3               	sta (scr_tmp),y		; store screenbyte
001005r 3               	php			; save carry
001005r 3               
001005r 3               	sec			; subtract 16 form screenaddress
001005r 3               	lda scr_tmp
001005r 3               	sbc #$10
001005r 3               	sta scr_tmp
001005r 3               	bcs :+
001005r 3               	dec scr_tmp+1
001005r 3               :
001005r 3               	plp			; restore carry
001005r 3               	dex			; previous column
001005r 3               	bpl scrly0		; repeat for all columns
001005r 3               
001005r 3               	inc line_cnt		; next line
001005r 3               	lda line_cnt
001005r 3               	cmp #8
001005r 3               	bne scrly1		; repeat for 8 lines
001005r 3               
001005r 3               ; Get next character in string
