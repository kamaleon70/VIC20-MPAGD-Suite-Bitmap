0000B6r 3               :
0000B6r 3               plot0:
0000B6r 3               	lda dispx		; which pixel within byte do we
0000B6r 3               	and #7			; want to set first?
0000B6r 3               	tay
0000B6r 3               	lda dots,y 		; table of small pixel positions.
0000B6r 3               	sta z80_e 		; get value.
0000B6r 3               
0000B6r 3               	jsr scadd 		; screen address.
0000B6r 3               	ldy #0
