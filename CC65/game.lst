000290r 3               ;-----------------------------------------------
000290r 3               ; Seek objects at sprite position.
000290r 3               ;
000290r 3               ; Output:
000290r 3               ;  A = object number, if not found A=255
000290r 3               ;-----------------------------------------------
000290r 3               
000290r 3               skobj:
000290r 3               	lda #<objdta 		; pointer to objects.
000290r 3               	sta z80_l
000290r 3               	lda #>objdta
000290r 3               	sta z80_h
000290r 3               
000290r 3               	lda numob 		; number of objects in game.
000290r 3               	sta z80_b 		; set up the loop counter.
000290r 3               skobj0:
000290r 3               	lda scno		; current room number.
000290r 3               	ldy #33
000290r 3               	cmp (z80_hl),y		; is object in here?
000290r 3               	bne :+
000290r 3               	jsr skobj1		; yes, check coordinates.
000290r 3               :
000290r 3               	clc			; point to next object in table.
000290r 3               	lda z80_l
000290r 3               	adc #39			; size of each object.
000290r 3               	sta z80_l
000290r 3               	bcc :+
000290r 3               	inc z80_h
000290r 3               :
000290r 3               	dec z80_b
000290r 3               	bne skobj0		; repeat for all objects.
000290r 3               
000290r 3               	lda #255		; end of list and nothing found, return 255.
000290r 3               	rts
000290r 3               
000290r 3               skobj1:
000290r 3               	ldy #34			; point to y coordinate.
000290r 3               	lda (z80_hl),y		; point to y coordinate.
000290r 3               	sec
000290r 3               	ldy #var_newY
000290r 3               	sbc (z80_ix),y 		; subtract sprite y.
000290r 3               	clc
000290r 3               	adc #15			; add sprite height minus one.
000290r 3               	cmp #31			; within range?
000290r 3               	bcs skobj2		; no, ignore object.
000290r 3               
000290r 3               	ldy #35			; point to x coordinate now.
000290r 3               	lda (z80_hl),y 		; get coordinate.
000290r 3               	sec
000290r 3               	ldy #var_newX
000290r 3               	sbc (z80_ix),y 		; subtract the sprite x.
000290r 3               	clc			; add sprite width minus one.
000290r 3               	adc #15
000290r 3               	cmp #31			; within range?
000290r 3               	bcs skobj2		; no, ignore object.
000290r 3               
000290r 3               	pla			; remove return address from stack.
000290r 3               	pla
000290r 3               
000290r 3               	lda numob 		; objects in game.
000290r 3               	sec
000290r 3               	sbc z80_b		; subtract loop counter.
000290r 3               skobj2:
000290r 3               	rts			; accumulator now points to object.
000290r 3               .endif
