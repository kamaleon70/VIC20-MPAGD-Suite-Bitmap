001005r 3               scrly:
001005r 3               
001005r 3               ; Set scr_l = txtscr
001005r 3               
001005r 3               	rts
001005r 3               	.word txtscr         	; set scr_l = left screen address.
001005r 3               	sta scr_l
001005r 3               	sta scr_r
001005r 3               	lda txtscr+1
