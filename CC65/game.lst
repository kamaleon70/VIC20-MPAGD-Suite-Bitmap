001005r 3               
001005r 3               	stx xtmp		; save x
001005r 3               
001005r 3               ; Calculate scr_r = scr_l + 16*(txtwid -1)
001005r 3               
001005r 3               	ldx txtwid
001005r 3               	dex
001005r 3               	lda scrollpos_lb,x
001005r 3               	clc
001005r 3               	adc scr_r
001005r 3               	sta scr_r		; set lb scr_r = scr_l + (txtwid - 1) * 8
001005r 3               	bcc :+
001005r 3               	inc scr_r+1
