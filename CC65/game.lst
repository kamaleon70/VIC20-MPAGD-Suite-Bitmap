001004r 3               
001004r 3               digd:
001004r 3               	ldy #9
001004r 3               	lda (z80_ix),y
001004r 3               	sta dispx		; set y
001004r 3               
001004r 3               	dey
001004r 3               	clc
001004r 3               	lda (z80_ix),y
001004r 3               	adc #16
