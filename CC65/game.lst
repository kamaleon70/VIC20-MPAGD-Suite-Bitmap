001004r 3               
001004r 3               	lsr txtbit		; bit of text to display.
001004r 3               	bcs :+
001004r 3               	rts
001004r 3               :
001004r 3               	ldy #0
001004r 3               	lda (scr_txt),y 	; what was the character?
