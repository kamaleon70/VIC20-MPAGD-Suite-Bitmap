000095r 3               dotr:
000095r 3               	ldy #5
000095r 3               	lda (z80_ix),y
000095r 3               	clc
000095r 3               	adc #1		 	; move left.
000095r 3               	sta (z80_ix),y
000095r 3               	rts
000095r 3               dotu:
000095r 3               	ldy #3
000095r 3               	lda (z80_ix),y
000095r 3               	sec
