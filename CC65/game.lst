000096r 3               
000096r 3               ;----------------------------------------------------
000096r 3               ; Create a vertical or horizontal star.
000096r 3               ;----------------------------------------------------
000096r 3               
000096r 3               star:
000096r 3               	lda z80_i		; store pointer to sprite.
000096r 3               	pha
000096r 3               	lda z80_x
000096r 3               	pha
000096r 3               
000096r 3               	jsr fpslot 		; find particle slot.
000096r 3               	bcs star7		; found one we can use.
000096r 3               star0:
