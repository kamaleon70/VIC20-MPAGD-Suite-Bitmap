000FEDr 3               	bcc :+
000FEDr 3               	lda #$60
000FEDr 3               	jmp iscrl0		; no, disable messages.
000FEDr 3               :
000FEDr 3               	ldx z80_b		; message number.
000FEDr 3               	jsr getwrd 		; find message start.
000FEDr 3               
000FEDr 3               	lda z80_l		; set initial text position.
000FEDr 3               	sta txtini
000FEDr 3               	lda z80_h
000FEDr 3               	sta txtini+1
000FEDr 3               
000FEDr 3               	lda #$ad		; code for lda adrr
000FEDr 3               iscrl0:
000FEDr 3               	sta scrly		; enable/disable scrolling routine.
000FEDr 3               
000FEDr 3               	jsr prescr 		; set up display position.
000FEDr 3               	jsr gprad 		; get print address.
000FEDr 3               
000FEDr 3               	lda scraddr 		; set text screen address.
000FEDr 3               	sta txtscr
000FEDr 3               	lda scraddr+1
000FEDr 3               	sta txtscr+1
000FEDr 3               
000FEDr 3               	lda z80_c		; width.
000FEDr 3               	sta txtwid		; set width in working storage.
000FEDr 3               
000FEDr 3               	lda #128 		; start with leftmost bit.
000FEDr 3               	sta txtbit
000FEDr 3               
000FEDr 3               	jmp scrly4
000FEDr 3               .endif
