0002A4r 3               ;-----------------------------------------------
0002A4r 3               
0002A4r 3               skobj:
0002A4r 3               	lda #<objdta 		; pointer to objects.
0002A4r 3               	sta z80_l
0002A4r 3               	lda #>objdta
0002A4r 3               	sta z80_h
0002A4r 3               
0002A4r 3               	lda numob 		; number of objects in game.
0002A4r 3               	sta z80_b 		; set up the loop counter.
0002A4r 3               skobj0:
0002A4r 3               	lda scno		; current room number.
0002A4r 3               	ldy #33
0002A4r 3               	cmp (z80_hl),y		; is object in here?
0002A4r 3               	bne :+
0002A4r 3               	jsr skobj1		; yes, check coordinates.
0002A4r 3               :
0002A4r 3               	clc			; point to next object in table.
0002A4r 3               	lda z80_l
0002A4r 3               	adc #39			; size of each object.
0002A4r 3               	sta z80_l
0002A4r 3               	bcc :+
0002A4r 3               	inc z80_h
0002A4r 3               :
0002A4r 3               	dec z80_b
0002A4r 3               	bne skobj0		; repeat for all objects.
