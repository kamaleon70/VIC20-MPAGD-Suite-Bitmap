001004r 3               	bne :+
001004r 3               	rts
001004r 3               :
001004r 3               	clc
001004r 3               	lda dispx		; look 1 cell down
001004r 3               	adc #8
001004r 3               	sta dispx
001004r 3               	jsr tstbl		; check blocktype in MAP
