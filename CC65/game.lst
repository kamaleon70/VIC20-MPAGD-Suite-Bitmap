000094r 3               proshr:
000094r 3               .if pflag
000094r 3               	lda #<SHRAPN		; table.
000094r 3               	sta z80_x
000094r 3               	lda #>SHRAPN
000094r 3               	sta z80_i
000094r 3               
000094r 3               	lda #NUMSHR		; shrapnel pieces to process.
000094r 3               	sta shrctr
000094r 3               prosh0:
000094r 3               	ldy #0
000094r 3               	lda (z80_ix),y		; on/off marker.
000094r 3               	asl a
000094r 3               proshx:
000094r 3               	bcs :+
000094r 3               	jsr prosh1 		; on, so process it.
000094r 3               :
000094r 3               	clc
000094r 3               	lda z80_x
000094r 3               	adc #SHRSIZ
000094r 3               	sta z80_x
000094r 3               	bcc :+
000094r 3               	inc z80_i
