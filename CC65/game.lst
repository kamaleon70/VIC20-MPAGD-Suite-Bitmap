001005r 3               	lda txtpos 		; get text pointer.
001005r 3               	sta scr_txt
001005r 3               	lda txtpos+1
001005r 3               	sta scr_txt+1
001005r 3               
001005r 3               	ldy #0
001005r 3               	lda (scr_txt),y 	; find character we're displaying.
001005r 3               	and #127 		; remove end marker bit if applicable.
001005r 3               	cmp #13			; is it newline?
001005r 3               	bne scrly5 		; no, it's okay.
001005r 3               	lda #32			; convert to a space instead.
001005r 3               scrly5:
001005r 3               	sta fntaddr		; calculate char address
001005r 3               	lda #0
001005r 3               	sta fntaddr+1
001005r 3               	asl fntaddr  		; multiply char by 8.
001005r 3               	rol fntaddr+1
001005r 3               	asl fntaddr
001005r 3               	rol fntaddr+1
001005r 3               	asl fntaddr
001005r 3               	rol fntaddr+1
001005r 3               	lda fntaddr
001005r 3               	clc
001005r 3               	adc #<(FONT-256)
