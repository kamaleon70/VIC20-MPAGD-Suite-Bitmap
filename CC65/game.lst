001007r 3               	cpy #8
001007r 3               	bne scrly3
001007r 3               
001007r 3               	lsr txtbit		; bit of text to display.
001007r 3               	bcs :+
001007r 3               	rts
001007r 3               :
001007r 3               	ldy #0
