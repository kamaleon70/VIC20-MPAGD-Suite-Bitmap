000011r 3               ; Modify for inventory.
000011r 3               ; called by the INV command
000011r 3               ;
000011r 3               ; Input:
000011r 3               ;  X   = message nr with objects seperated with ,
000011r 3               ;
000011r 3               ; Output:
000011r 3               ;  OPT = selected line nr of INV menu
000011r 3               ;--------------------------------------------------------------
000011r 3               
000011r 3               .if mflag
000011r 3               minve:
000011r 3               .if xflag
000011r 3               	lda #WINDOWHGT
000011r 3               	asl a
000011r 3               	sta winhgt
000011r 3               	lda #WINDOWWID
000011r 3               	asl a
000011r 3               	sta winwid
000011r 3               .endif
000011r 3               	lda #<(invdis)		; routine address.
000011r 3               	sta mod0+1		; set up menu routine.
000011r 3               	sta mod2+1		; set up count routine.
000011r 3               	lda #>(invdis)
000011r 3               	sta mod0+2
000011r 3               	sta mod2+2
000011r 3               	lda #<(fopt)		; find option from available objects.
000011r 3               	sta mod1+1		; set up routine.
000011r 3               	lda #>(fopt)
000011r 3               	sta mod1+1+1
000011r 3               	jmp dbox		; do menu routine.
